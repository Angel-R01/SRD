# Let NetworkManager manage all devices on this system
network:
  version: 2
  renderer: NetworkManager
  ethernets:
    enp0s3:
      dhcp4: false
      addresses:
        - 192.168.15.10
      gateway4: 192.168.19.100
      nameservers:
        search: [angelrr.edu]
        addresses: [192.168.15.200]


